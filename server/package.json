{
    "name": "server-kanban-board-with-jwt-auth",
    "version": "1.0.0",
    "description": "Server for the JWT Auth Kanban Board",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "build": "npx tsc",
        "start": "npm run build && node dist/server.js",
        "dev": "npx nodemon",
        "seed": "node dist/seeds/index.js",
        "clean": "rimraf dist"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts,json,js",
        "exec": "npx tsc && node dist/server.js"
    },
    "keywords": [
        "server",
        "jwt",
        "kanban",
        "typescript"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.1",
        "pg": "^8.11.0",
        "sequelize": "^6.32.1",
        "typescript": "^5.8.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jsonwebtoken": "^9.0.1",
        "rimraf": "^6.0.1"
    }
}
